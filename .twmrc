menu "todie"
{
	"Are you fucking sure?" ("gray80":"#773333") f.title
	"No" f.nop
	"Yes" f.delete
}

menu "main"
{
"T"  ("gray80":"#773333")	f.title
"Raise"		f.raise
"Lower"		f.lower
"Focus"		f.focus
"Unfocus"	f.unfocus
"Move"		f.move
""	f.nop
"Show Iconmgr"	f.showiconmgr
"Hide Iconmgr"	f.hideiconmgr
#"WorkSpace manager" f.showworkspacemgr
"Etc"			f.title
"Date"		f.exec "xmessage `date` &"
"LeftHand" f.exec "setxkbmap us dvorak-l &"
"Dvorak"   f.exec "setxkbmap dvorak &"

"Kill"	("gray80":"#773333") f.menu "todie"
""	f.nop

"Exit"	("gray80":"#773333")	f.menu "exit"
}


menu "exit"
{
"Are you sure ?" ("gray80":"#773333") f.title
"No"	f.nop
"Restart" f.restart 
"Yes"	f.quit
}

menu "exec"
{
":: Applications ::"	 ("gray80":"#773333") f.title
"Xfce4 Ternimal"	f.exec "exec xfce4-terminal &"
"Gnome Terminal"  f.exec "exec gnome-terminal &"
"Xterm"		f.exec "exec /usr/bin/xterm &"
"Konsole KDE's console"	f.exec "konsole &"
"Chrome"	f.exec "exec google-chrome --user-data-dir=/home/guu &"
"chromium-browser"  f.exec "exec chromium-browser &"

"Firefox"	f.exec "exec /usr/bin/firefox &"
"Opera"		f.exec "opera &"
"Konqueror"	f.exec "konqueror &"
"KMail"		f.exec "kmail &"
"Blender"	f.exec "blender &"
"Gimp"		f.exec "gimp &"
"LibreCAD" f.exec "librecad &"
"OpenSCAD" f.exec "openscad &"
"Xmag"	f.exec "exec /usr/bin/xmag &"
"XClock"	f.exec "exec /usr/bin/xclock -update 1 &"
"XFig"	f.exec "exec /usr/bin/xfig -nosplash &"
"Xcompmgr"	f.exec "exec /usr/bin/xcompmgr -cC -t-6 -l-6 -r4 &"
": Chat Applications :"	f.title
"Konversation"		f.exec "konversation &"

": Editors : "	f.title
"Kate"		f.exec "kate &"
"GVim"		f.exec "gvim &"
"Geany"		f.exec "geany &"
"KWrite"		f.exec "kwrite &"
"Gedit"		f.exec "gedit &"
"leafpad"	f.exec "leafpad &"


": Commercial Editors : "	f.title
"Sublime3"  f.exec "subl &"


": Virtual Software : " f.title
"Vmware"	f.exec "vmware &"
"My Computer"	f.title

#"Home"	f.exec "nemo  --no-desktop ~/ &"
#"Home"	f.exec "dolphin ~/ &"
"Home"	f.exec "/usr/bin/nautilus --new-window  ~/ &"

": Daily :" f.title
"RedNoteBook"	f.exec 	"rednotebook &"
": Compare :" f.title
"Meld"		f.exec "meld &"

}

WindowRing
{
"xterm"
"rxvt"
"urxvt"

}

"x" = m | s: all : f.delete
"i" = m | s: all : f.iconify
"t" = m | c: all : f.exec "exec /usr/bin/xterm +sb &"
"c" = m | c: all : f.exec "exec utf8 /usr/bin/xterm -fa nSimSun -fs 8 +sb &"
"k" = m | c: all : f.exec "exec /usr/bin/sunclock &"
"g" = m | c: all : f.exec "exec /usr/bin/gimp &"
"p" = m | c: all : f.exec "exec /usr/bin/xpad &"
"l" = m | c: all : f.exec "exec xlock -bg black -fg '#aaa' &"
"F12" = m : all : f.fullzoom

#Ctrl+button3 ,appear a prg menu every where
#Button3 = c : all : f.menu "prg" 


Function "winup" { f.circleup }
"Tab" = m : root|window|frame|title : f.function "winup"

